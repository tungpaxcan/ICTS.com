        <script type="text/javascript">
            function onChangeLanguage() {
                $.post('/LoadData/LoadInformation.aspx', { type: "changelanguage", Language: $("#cbLanguage").val() }, function (data) {
                    document.location.reload();
                });
            }
            $(function () {
                var language = '';
                if (language == "en") {
                    $('#cbLanguage').val('en');
                }
                else {
                    $('#cbLanguage').val('vn');
                }
            });
            $("#btnRegisterEmail").click(function () {
                if ($("#txtRegisterEmailWebsite").val() == "") {
                    Alert('Vui lòng nhập email của bạn', 'warning');
                    return false;
                }
                if (checkEmail('txtRegisterEmailWebsite') == false) {
                    Alert('Email không đúng, vui lòng kiểm tra lại.', 'warning');
                    return false;
                }
                $("#showloading").show();
                $.post('/LoadData/LoadInformation.aspx', { type: "registeremail", Email: document.getElementById("txtRegisterEmailWebsite").value }, function (data) {
                    Alert(data, 'success');
                    document.getElementById("txtRegisterEmailWebsite").value = "";
                    $("#showloading").hide();
                });
            });
            $('#txtRegisterEmailWebsite').keypress(function (e) {
                if (e.which == 13) {
                    $("#btnRegisterEmail").click();
                    return false;
                }
            });
            $(document).ready(function () {
                $("#btnSearch").click(function () {
                    if ($("#txtSearch").val() == "") {
                        Alert('Bạn chưa nhập nội dung tìm kiếm!', 'warning');
                        return false;
                    }
                    else {
                        var content = encodeURIComponent(ReplaceSpecialCharacter(document.getElementById("txtSearch").value.trim()));
                        document.location = "/Product/search=" + content;
                    }
                });
                $('#txtSearch').keypress(function (e) {
                    if (e.which == 13) {
                        $("#btnSearch").click();
                        return false;
                    }
                });
            });

            //Contact business
            $.ajax({
                url: '/home/TTLHKD',
                type: 'get',
                success: function (data) {
                    $('#ttlhkd').empty();
                    if (data.code == 200) {
                        $.each(data.ttlh, function (k, v) {
                            let i = '<div id="touchFlow2" class="nav_h_type">';
                            i += '<ul>';
                            i += '<li>';
                            i += ' <a href="/Upload/Content/Certification/01422019034232864_71.jpg"data-fancybox="gallery1" data-caption="CN5" tabindex="1">';
                            i += '<a id="' + v.id + '"  style="font-size:16px;color:black" class="hotline" href="http://haphan.com/AboutUs" target="_blank">';
                            i += '<i class="fa-solid fa-headset">' + v.phone + '(' + v.name + ')</i>';
                            i += '</a>';
                            i += '</a>';
                            i += '</li>';
                            i += '</ul>';
                            i += '</div>';
                            $('#ttlhkd').append(i);
                        });
                    }
                }
            });
            //CSKH
            $.ajax({
                url: '/home/CSKH',
                type: 'get',
                success: function (data) {
                    $('#cskh').empty();
                    if (data.code == 200) {
                        $.each(data.ttlh, function (k, v) {
                            let i = '<div id="touchFlow2" class="nav_h_type">';
                            i += '<ul>';
                            i += '<li>';
                            i += ' <a href="/Upload/Content/Certification/01422019034232864_71.jpg"data-fancybox="gallery1" data-caption="CN5" tabindex="1">';
                            i += '<a style="color:black" class="hotline" href="http://haphan.com/AboutUs" target="_blank">';
                            i += '<i class="fa-solid fa-headset">' + v.phone + '(' + v.name + ')</i>';
                            i += '</a>';
                            i += '</a>';
                            i += '</li>';
                            i += '</ul>';
                            i += '</div>';
                            $('#cskh').append(i);
                        });
                    }
                }
            });
            //logo
            $.ajax({
                url: '/home/Logo',
                type: 'get',
                success: function (data) {
                    $('#logo').empty();
                    if (data.code == 200) {
                        $.each(data.logo, function (k, v) {
                            let i = '<a href="/" id="' + v.id + '" >';
                            i += '<img style="width:200px;margin-left:-100px" src="/Images/' + v.image + '" alt="' + v.title + '" />';
                            i += ' </a>';
                            $('#logo').append(i);
                        });
                    }
                }
            });
            //footerleft
            $.ajax({
                url: '/icts/footers/FooterLeft',
                type: 'get',
                success: function (data) {
                    $('#footerleft').empty();
                    if (data.code == 200) {
                        $.each(data.footerleft, function (k, v) {
                            let div = '<div id="' + v.id + '" class="h-addressa">';
                            div += '<h3>' + v.name + '</h3>';
                            div += '</div>';
                            $('#footerleft').append(div);
                        });
                    }
                }
            });
            //footercenter
            $.ajax({
                url: '/icts/footers/FooterCenter',
                type: 'get',
                success: function (data) {
                    $('#footercenter').empty();
                    if (data.code == 200) {
                        $.each(data.footercenter, function (k, v) {
                            let div = '<div id="' + v.id + '" class="h-addressa">';
                            div += '<img  src="/Images/' + v.image + '" />';
                            div += '</div>';
                            $('#footercenter').append(div);
                        });
                    }
                }
            });
            //footerright
            $.ajax({
                url: '/home/FooterRight',
                type: 'get',
                success: function (data) {
                    $('#footerright').empty();
                    if (data.code == 200) {
                        $.each(data.footerright, function (k, v) {
                            let div = '<div id="' + v.id + '" class="h-contact">';
                            div += '<h3>' + v.name + '</h3>';
                            div += '<h4>' + v.address + '</h4>';
                            div += '<h4>' + v.phone + '</h4>';
                            div += '</div>';
                            $('#footerright').append(div);
                            $('#emaillayout').empty();
                            let email = '<h3>' + v.email + '</h3>';
                            $('#emaillayout').append(email);
                        });
                        $.ajax({
                            url: '/home/FooterRight',
                            type: 'get',
                            success: function (data) {
                                $('#iconfooter').empty();
                                if (data.code == 200) {
                                    $.each(data.footerright2, function (k, v) {
                                        let ul = '<li id="' + v.id + '">';
                                        ul += '<a style="color:white" href="' + v.link + '" target = "_blank" >';
                                        ul += '<i class="' + v.dec + '"></i>';
                                        ul += '</a>';
                                        ul += '</li>';
                                        $('#iconfooter').append(ul);
                                    });
                                }
                            }
                        });
                    }
                }
            });
            //menusanpham
            $.ajax({
                url: '/categorieshome/DanhMucSanPham',
                type: 'get',
                success: function (data) {
                    $('#menusanpham').empty();
                    if (data.code == 200) {
                        let li = '<p class="menuct">Danh Mục</p>';
                        $.each(data.dmsp, function (k, v) {

                            li += '<li class="nav-nav">';
                            li += '<a name="acate" id="cate-' + v.id + '" class="smooth cate" href="" title="">' + v.name + ' <i class="catei' + v.id + ' fa-duotone fa-chevrons-right"></i></a >';
                            li += '<ul class="categoryproduct' + v.id + '">';
                            li += '</ul>';
                            li += '</li>';
                           
                        }); $('#menusanpham').append(li);
                       
                    }
                    let idscate = $('.cate').map(function (_, x) { return x.id; }).get();
                    for (let i = 0; i < idscate.length; i++) {
                        let id = idscate[i].substring(5)
                        $('#' + idscate[i] + '').mouseenter(function () {                                                    
                            $('.categoryproduct' + id + '').addClass('Add' + idscate[i] + '');
                            $('#menusanpham .catei' + id + '').css("visibility", "visible")
                            $.ajax({
                                url: '/categorieshome/ProductCategory/',
                                type: 'get',
                                data: {
                                    id: id
                                },
                                success: function (data) {
                                    $('.Add' + idscate[i] + '').empty();
                                    if (data.code == 200) {
                                       
                                        let li = '<p class="menuct">Loại Sản Phẩm</p>';
                                        $.each(data.dmsp, function (k, v) {                                          
                                            li += '<li>';
                                            li += '<a href="" id="pro-' + v.id + '" name="apro" class="pro">' + v.name + ' <i class="proi' + v.id + ' fa-duotone fa-chevrons-right"></i></a>';
                                            li += '<ul class="product' + v.id + '">';
                                            li += '</ul>';
                                            li += '</li>';
                                            
                                        }); $('.Add' + idscate[i] + '').append(li);
                                    }
                                    let idspro = $('.pro').map(function (_, x) { return x.id; }).get();
                                    for (let i = 0; i < idspro.length; i++) {
                                        let id = idspro[i].substring(4)
                                        $('#' + idspro[i] + '').mouseenter(function () {
                                            $('.product' + id + '').addClass('Add' + idspro[i] + '');
                                            $('#menusanpham .proi' + id + '').css("visibility", "visible")
                                            $.ajax({
                                                url: '/categorieshome/Product/',
                                                type: 'get',
                                                data: {
                                                    id: id
                                                },
                                                success: function (data) {
                                                    $('.Add' + idspro[i] + '').empty();
                                                    if (data.code == 200) {
                                                        let li = '<p class="menuct">Sản Phẩm</p>';
                                                        $.each(data.dmsp, function (k, v) {
                                                             li += '<li>';
                                                            li += '<a href="" id="pro-' + v.id + '" name="bproduct" class="pro">' + v.name + '</a>';
                                                            li += '</li>';
                                                           
                                                        }); $('.Add' + idspro[i] + '').append(li);
                                                    }
                                                }
                                            });
                                        })
                                        $('#' + idspro[i] + '').mouseleave(function () {
                                            $('#menusanpham .proi' + id + '').css("visibility", "hidden")
                                        })
                                    }
                                }                            
                            });
                        })
                        $('#' + idscate[i] + '').mouseleave(function () {
                            $('#menusanpham .catei' + id + '').css("visibility", "hidden")
                        })
                    }
                    
                }
            });           
            //menuservice
            $.ajax({
                url: '/service/ServiceMenu',
                type: 'get',
                success: function (data) {
                    $('#menuservice').empty();
                    if (data.code == 200) {
                        let li = '<p class="menuct">Dịch Vụ</p>';
                        $.each(data.dmsp, function (k, v) {
                            li += '<li class=""nav-nav>';
                            li += '<a href="" name="aservice" id="' + v.id + '" class="smooth" href="" title="' + v.title + '">' + v.name + '</a >';
                            li += '</li>';
                        }); $('#menuservice').append(li);
                    }
                }
            });
            //link service
            $(document).on('click', "a[name='aservice']", function () {
                let idservice = $(this).closest('a').attr('id');
                $.ajax({
                    url: '/service/ServiceDetail',
                    type: 'get',
                    data: {
                        id: idservice
                    },
                    success: function (data) {
                        if (data.code == 200) {
                            $.each(data.dmsp, function (k, v) {
                                location.href = data.Url + v.meta + "/" + v.id;
                            })
                        }
                    }
                });
            })
            //link cate
            $(document).on('click', "a[name='acate']", function () {
                let idcate = $(this).closest('a').attr('id');
                let idcate2 = idcate.substring(5);
                $.ajax({
                    url: '/categoryproducthome/Cate',
                    type: 'get',
                    data: {
                        id: idcate2
                    },
                    success: function (data) {
                        if (data.code == 200) {
                            $.each(data.dmsp, function (k, v) {
                                location.href = data.Url + v.meta.replace(/&|\//g,"") + "/" + v.id;
                            })
                        }
                    }
                });
            })
            //link procate
            $(document).on('click', "a[name='apro']", function () {
                let idpro = $(this).closest('a').attr('id');
                let idprocate = idpro.substring(4);
                $.ajax({
                    url: '/categoryproducthome/ProCateUrl',
                    type: 'get',
                    data: {
                        id: idprocate
                    },
                    success: function (data) {
                        if (data.code == 200) {
                            $.each(data.dmsp, function (k, v) {
                                location.href = data.Url + v.meta.replace(/&|\//g, "") + "/" + v.id;
                            })
                        }
                    }
                });
            })
            /* ---------addlinkproduct---------------*/
            $(document).on('click', "a[name='bproduct']", function () {
                let idp = $(this).closest('a').attr('id');
                let idproduct = idp.substring(4)
                $.ajax({
                    url: '/producthome/DetailProductById',
                    type: 'get',
                    data: {
                        id: idproduct
                    },
                    success: function (data) {
                        if (data.code == 200) {
                            $.each(data.left, function (k, v) {
                                window.location.href = data.Url + v.meta + "/" + v.id
                            })

                        }
                    }
                })
            })
            //menusolution
            $.ajax({
                url: '/solutionhome/Solution',
                type: 'get',
                success: function (data) {
                    $('#menusolution').empty();
                    if (data.code == 200) {
                        let li = '<p class="menuct">Giải Pháp</p>';
                        $.each(data.left, function (k, v) {                            
                             li += '<li class="nav-nav">';
                            li += '<a href="" name="asolution" id="' + v.id + '" class="smooth"  title="' + v.title + '">' + v.name + '</a >';
                            li += '</li>';                           
                        }); $('#menusolution').append(li);
                    }
                }
            });
            //link solution
            $(document).on('click', "a[name='asolution']", function () {
                let idsolution = $(this).closest('a').attr('id');
                $.ajax({
                    url: '/solutionhome/DetailSolution',
                    type: 'get',
                    data: {
                        id: idsolution
                    },
                    success: function (data) {
                        if (data.code == 200) {
                            $.each(data.left, function (k, v) {
                                location.href = data.Url + v.meta + "/" + v.id;
                            })
                        }
                    }
                });
            })
        </script>
        <script src="~/Scripts/js/script.js"></script>
        <script src="~/Content/bootstrap-4.1.3/bootstrap-4.1.3/dist/js/bootstrap.js"></script>
        @RenderSection("scripts", required: false)
</body>
</html>
