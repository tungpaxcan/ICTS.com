                    if (data.code == 200) {
                        $.each(data.left, function (k, v) {
                            window.location.href = data.Url + v.meta + "/" + v.id

                        })

                    }
                }
            })
        })
        /*        -----------look------------------------*/
        $('#input').on('keyup', function (e) {
            var pagel = 1;
            var none = $('#input').val();
            autoseach(none, pagel);
            if (none.length == 0) {
                none = "";
                $('#tbdlook').css("display", "none");
                $('#pagel').css("display", "none");
            }
            $('#nextl').click(function () {
                pagel += 1;
                autoseach(none, pagel);
            })
            $('#previousl').click(function () {
                pagel -= 1;
                autoseach(none, pagel);
            })
        });
        function autoseach(none, pagel) {
            $('#tbdlook').css("display", "block");
            $('#pagel').css("display", "block");
            $.ajax({
                url: '/producthome/ProductLook',
                type: 'get',
                data: {
                    seach: none,
                    page: pagel
                },
                success: function (data) {
                    $('#tbdlook').empty();
                    if (data.code == 200) {
                        $.each(data.dmsp, function (k, v) {
                            let s = '<tr>';
                            s += '<td><img src="/Images/' + v.image + '"/></td>';
                            s += '<td><a id="pro-' + v.id + '" name="bproduct">' + v.name + '</a></td>';
                            s += '</tr>';
                            $('#tbdlook').append(s);
                        });
                    }
                }
            });
        }
        function googleTranslateElementInit() {
            new google.translate.TranslateElement(
                {
                    pageLanguage: "vi",
                    layout: google.translate.TranslateElement.InlineLayout.SIMPLE
                },
                "google_translate_element"
            );
        }
    </script>
