                        table += '<tbody>';
                        $.each(data.dmsp, function (k, v) {
                            table += '<tr id="' + v.id + '">';
                            table += '<td>' + v.nameproductcategory + '</td>';
                            table += '<td>' + v.namebrand + '</td>';
                            table += '<td>' + v.name + '</td>';
